  <!-- start log4net settings -->
  <log4net>
    <logger name="Amazon">
      <level value="OFF" />
    </logger>
    <root>
      <appender-ref ref="RollingLogFileAppender" />      
    </root>
    <appender name="RollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <threshold value="INFO" />
      <file value="E:\LogFiles\Errors\MNDayCare.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMM" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%date] - [%level] - [%logger] - %message%newline" />
      </layout>
    </appender>
    <appender name="EmailAppender" type="log4net.Appender.SmtpAppender">
      <evaluator type="log4net.Core.LevelEvaluator">
        <threshold value="DEBUG" />
      </evaluator>
      <filter type="log4net.Filter.StringMatchFilter">
        <stringToMatch value="SOME STRING TO MATCH" />
        <acceptOnMatch value="true" />
      </filter>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <acceptOnMatch value="true" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
      <subject value="MNDayCare Error" />
      <to value="maneesh@zitisi.com" />
      <from value="maneesh@zitisi.com" />      
      <bufferSize value="10" />
      <lossy value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date%newline%-5level %logger%newline%message%newline" />
      </layout>
    </appender>
  </log4net>
    <system.webServer>
